<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="vsdc_item_wizard_form" model="ir.ui.view">
            <field name="name">vsdc.item.wizard.form</field>
            <field name="model">vsdc.item.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="last_date"/>
                        </group>
                        <footer>
                            <button name="action_import" string="Import" type="object" default_focus="1"
                                    class="oe_highlight"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>